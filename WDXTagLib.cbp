<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="WDXTagLib" />
		<Option platforms="Windows;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option platforms="Windows;" />
				<Option output="Debug\WDXTagLibDebug.wdx" prefix_auto="1" extension_auto="0" />
				<Option object_output="Debug\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="TOTALCMD.EXE" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-DBUILD_DLL" />
					<Add option="-DDEBUG" />
					<Add directory="taglib\debug\include" />
				</Compiler>
				<Linker>
					<Add library="user32" />
					<Add library="taglib\debug\lib\libtagd.dll.a" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option platforms="Windows;" />
				<Option output="Release\WDXTagLib.wdx" prefix_auto="1" extension_auto="0" />
				<Option object_output="Release\" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
					<Add option="-DNDBUG" />
					<Add directory="taglib\release\include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="user32" />
					<Add library="taglib\release\lib\libtag.dll.a" />
				</Linker>
				<ExtraCommands>
					<Add before='SubWCRev.exe &quot;$(PROJECTDIR).&quot; &quot;$(PROJECTDIR)\src\VerInfo.rc.template&quot; &quot;$(PROJECTDIR)\src\VerInfo.rc&quot;' />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-DUNICODE_NO" />
			<Add option="-D_UNICODE_NO" />
		</Compiler>
		<Linker>
			<Add option="-Wl,--kill-at" />
		</Linker>
		<Unit filename="doc\read_before_install.txt" />
		<Unit filename="doc\readme.txt" />
		<Unit filename="src\CUtils.cpp" />
		<Unit filename="src\CUtils.h" />
		<Unit filename="src\CWDXBase.cpp" />
		<Unit filename="src\CWDXBase.h" />
		<Unit filename="src\CWDXTagLib.cpp" />
		<Unit filename="src\CWDXTagLib.h" />
		<Unit filename="src\CustomTypes.h" />
		<Unit filename="src\VerInfo.rc">
			<Option compilerVar="WINDRES" />
			<Option target="Release" />
		</Unit>
		<Unit filename="src\VerInfo.rc.template" />
		<Unit filename="src\contentplug.h" />
		<Unit filename="src\main.cpp" />
		<Unit filename="src\main.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
